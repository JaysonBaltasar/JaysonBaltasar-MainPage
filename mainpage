top and bot container
import '../App.css'
function Right() {
    return (
        <div className='right'>
            <div className='top'>
                <h4>TOP CONTAINER</h4>
            </div>  
            <div className='below'>
                <h5>BELOW CONTAINER</h5>
            </div>
        </div>
    )
}


export default Right;

main.jsx
import React from 'react';
